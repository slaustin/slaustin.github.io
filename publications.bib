  keywords = {acanthocystis turfacea,channels,chlorella viruses,deuterium order parameter,dimethylsulfoxide,function,genome sequence,gromacs,lipid bilayer structure,mass density profiles,membrane proteins,molecular dynamics,phycodnaviridae,radial distribution,virus atcv-1,water orientation profile}
