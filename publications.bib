}

@article{malmstromAllosteryComputationalMicroscope2015,
  title = {Allostery through the Computational Microscope: {{cAMP}} Activation of a Canonical Signalling Domain},
  shorttitle = {Allostery through the Computational Microscope},
  author = {Malmstrom, Robert D. and Kornev, Alexandr P. and Taylor, Susan S. and Amaro, Rommie E.},
