@article{Kiyota2011,
  title = {A New Approach for Investigating the Molecular Recognition of Protein: {{Toward}} Structure-Based Drug Design Based on the {{3D-RISM}} Theory},
  author = {Kiyota, Yasuomi and Yoshida, Norio and Hirata, Fumio},
  year = {2011},
  journal = {Journal of Chemical Theory and Computation},
  volume = {7},
  number = {11},
  pages = {3803--3815},
  issn = {15499618},
  doi = {10.1021/ct200358h},
  abstract = {A new approach to investigate a molecular recognition process of protein is presented based on the three-dimensional reference interaction site model (3D-RISM) theory, a statistical mechanics theory of molecular liquids. Numerical procedure for solving the conventional 3D-RISM equation consists of two steps. In step 1, we solve ordinary RISM (or 1D-RISM) equations for a solvent mixture including target ligands in order to obtain the density pair correlation functions (PCF) among molecules in the solution. Then, we solve the 3D-RISM equation for a solute-solvent system to find three-dimensional density distribution functions (3D-DDF) of solvent species around a protein, using PCF obtained in the first step. A key to the success of the method was to regard a target ligand as one of "solvent" species. However, the success is limited due to a difficulty of solving the 1D-RISM equation for a solvent mixture, including large ligand molecules. In the present paper, we propose a method which eases the limitation concerning solute size in the conventional method. In this approach, we solve a solute-solute 3D-RISM equations for a protein-ligand system in which both proteins and ligands are regarded as "solutes" at infinite dilution. The 3D- and 1D-RISM equations are solved for protein-solvent and ligand-solvent systems, respectively, in order to obtain the 3D- and 1D-DDF of solvent around the solutes, which are required for solving the solute-solute 3D-RISM equation. The method is applied to two practical and noteworthy examples concerning pharmaceutical design. One is an odorant binding protein in the Drosophila melanogaster, which binds an ethanol molecule. The other is phospholipase A2, which is known as a receptor of acetylsalicylic acid or aspirin. The result indicates that the method successfully reproduces the binding mode of the ligand molecules in the binding sites measured by the experiments. {\copyright} 2011 American Chemical Society.}
}

@article{Kneller2002,
  title = {An Effective Method for the Discrimination of Motional Anisotropy and Chemical Exchange},
  author = {Kneller, Julie M. and Lu, Min and Bracken, Clay},
  year = {2002},
  journal = {Journal of the American Chemical Society},
  volume = {124},
  number = {9},
  pages = {1852--1853},
  issn = {00027863},
  doi = {10.1021/ja017461k},
  abstract = {Analysis of the ratio of transverse and longitudinal relaxation rates (R2/R1) is an approach commonly used for estimation of overall correlation time and identification of chemical exchange in biological macromolecules. However, this analysis fails to distinguish between chemical exchange and motional anisotropy. We describe a simple method for identifying chemical exchange and motional anisotropy using the product, R1R2. In the slow tumbling regime, the R1R2 product results in a constant value that is independent of overall correlation time and motional anisotropy. This analysis provides a simple method for rapidly estimating and dissociating the effects of motional anisotropy and chemical exchange in NMR heteronuclear spin relaxation data. We demonstrate the utility of the method with 15N relaxation data collected on the proteins E. coli ribonuclease H and the trimeric E. coli membrane associated lipoprotein lpp.},
  isbn = {0002-7863 (Print){\textbackslash}n0002-7863 (Linking)},
  pmid = {11866588},
  file = {/Users/stevenaustin/Zotero/storage/6JNE8B6I/ja017461k.pdf}
}

@article{knightLDynamicsFreeEnergy2009,
  title = {{$\lambda$}-{{Dynamics}} Free Energy Simulation Methods},
  author = {Knight, Jennifer L. and Brooks III, Charles L.},
  year = {2009},
  journal = {Journal of Computational Chemistry},
  volume = {30},
  number = {11},
  pages = {1692--1700},
  issn = {1096-987X},
  doi = {10.1002/jcc.21295},
  urldate = {2025-06-04},
  abstract = {Free energy calculations are fundamental to obtaining accurate theoretical estimates of many important biological phenomena including hydration energies, protein-ligand binding affinities and energetics of conformational changes. Unlike traditional free energy perturbation and thermodynamic integration methods, {$\lambda$}-dynamics treats the conventional ``{$\lambda$}'' as a dynamic variable in free energy simulations and simultaneously evaluates thermodynamic properties for multiple states in a single simulation. In the present article, we provide an overview of the theory of {$\lambda$}-dynamics, including the use of biasing and restraining potentials to facilitate conformational sampling. We review how {$\lambda$}-dynamics has been used to rapidly and reliably compute relative hydration free energies and binding affinities for series of ligands, to accurately identify crystallographically observed binding modes starting from incorrect orientations, and to model the effects of mutations upon protein stability. Finally, we suggest how {$\lambda$}-dynamics may be extended to facilitate modeling efforts in structure-based drug design. {\copyright} 2009 Wiley Periodicals, Inc. J Comput Chem 2009},
  copyright = {Copyright {\copyright} 2009 Wiley Periodicals, Inc.},
  langid = {english},
  keywords = {drug design,free energy,protein-ligand,sampling},
  file = {/Users/stevenaustin/Zotero/storage/KTDXTNDF/Knight and Brooks III - 2009 - λ-Dynamics free energy simulation methods.pdf;/Users/stevenaustin/Zotero/storage/IZI72N6H/jcc.html}
}

@article{Knothe1983,
  title = {Transferable Resistance to Cefotaxime, Cefoxitin, Cefamandole and Cefuroxime in Clinical Isolates of {{Klebsiella}} Pneumoniae and {{Serratia}} Marcescens},
  author = {Knothe, H. and Shah, P. and Krcmery, V. and Antal, M. and Mitsuhashi, S.},
  year = {1983},
  journal = {Infection},
  volume = {11},
  number = {6},
  pages = {315--317},
  issn = {03008126},
  doi = {10.1007/BF01641355},
  abstract = {In conjugational crosses, three Klebsiella pneumoniae strains and one Serratia marcescens strain have been demonstrated to transfer resistance determinants to newer types of cephalosporins. While Klebsiella strains donated cefotaxime, cefamandole and cefuroxime resistance to Escherichia coli K-12 recipients, the genetic analysis of exconjugants after the transfer of plasmids from Serratia strains to Proteus or Salmonella recipients showed that the cefoxitin resistance determinant was also co-transferred. In subsequent transfer cycles of this plasmid, cefotaxime and cefoxitin resistance determinants segregated in contrast to the relative stability of plasmids derived from Klebsiella strains in subsequent transfer cycles. From results obtained in this study, it may be concluded that in some strains of nosocomial Enterobacteriaceae, resistance to newer cephalosporins could be transmissible and thus plasmid-located. {\copyright} 1983 MMV Medizin Verlag GmbH M{\"u}nchen.},
  pmid = {6321357}
}

