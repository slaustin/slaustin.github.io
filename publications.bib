@article{kongLdynamicsNewApproach1996,
  title = {{$\lambda$}-Dynamics: {{A}} New Approach to Free Energy Calculations},
  shorttitle = {{$\lambda$}-Dynamics},
  author = {Kong, Xianjun and Brooks, Charles L.},
