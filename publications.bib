@article{wangMethylerythritolPhosphatePathway2018,
  title = {The {{Methylerythritol Phosphate Pathway}}: {{Promising Drug Targets}} in the {{Fight}} against {{Tuberculosis}}},
  shorttitle = {The {{Methylerythritol Phosphate Pathway}}},
