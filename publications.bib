@article{Davoudi2015,
  title = {Resistance {{Pattern}} of {{Antibiotics}} in {{Patient Underwent Open Heart Surgery With Nosocomial Infection}} in {{North}} of {{Iran}}},
  author = {Davoudi, Alireza and Najafi, Narges and Alian, Shahriar and Tayebi, Atefe and Ahangarkani, Fetemeh and Rouhi, Samaneh and Heydari, Amirhosein},
  year = {2015},
  journal = {Global Journal of Health Science},
  volume = {8},
  number = {2},
  pages = {288--297},
  issn = {1916-9744},
  doi = {10.5539/gjhs.v8n2p288},
  abstract = {BACKGROUND: Patients who undergo cardiac surgery appear to be at increased risk for the development of Nosocomial infections (NIs). The development of antibiotic-resistant infections has been associated with significantly greater hospital mortality rates compared to similar infections caused by antibiotic-sensitive pathogens.{\textbackslash}n{\textbackslash}nOBJECTIVES: The purpose of this study is survey of Nis and antibiotic resistance patterns of causative bacteria among patients who underwent open heart surgery in the north of Iran during a 2-year period between September 2012 and September 2014.{\textbackslash}n{\textbackslash}nMETHODS: In this cross-sectional study we evaluated 187 patients that underwent open heart surgery with NIs. Demographic feature, clinical characteristics and risk factors of each infection were recorded. The antibiotic susceptibility test was performed using the Minimum inhibitory concentration (MIC) method according to the standard protocol of Clinical \& Laboratory Standards Institute (CLSI). Detection of Extended-spectrum beta-lactamase (ESBL) producing bacteria was performed by the double-disk synergy (DDS) test; also Methicillin-resistant Staphylococcus (MRSA) strains were identified by MRSA Screen Agar. The collected data were analyzed using the SPSS software (ver. 16) and, descriptive statistics were used.{\textbackslash}n{\textbackslash}nRESULTS: Out Of total of 2253 hospitalized patients who underwent open heart surgery, 187(5.05\%) patients had NIs. 51.9\% of the patients were female. The rates of surgical site infection (SSI), respiratory tract infection, endocarditis, Urinary tract infection (UTI), blood Infection and mediastinitis were 27.80, 25.66\%, 17.64, 17.11\% 8.55\% and 3.20\% respectively. E.coli and S.aureus were the most causative agents of NIs. The rate of ESBL-producing bacteria was 14.28- 71.42\% among enterobacteriaceae and the rate of MRSA was 54.2\% among S.aureus strains. All isolated Acinetobacter.spp were Multi-drug resistance (MDR).{\textbackslash}n{\textbackslash}nCONCLUSIONS: We showed that the rate of NIs among these high-risk patients was in the average level. But the emergence of MRSA and ESBL bacteria is increasing in our region.},
  isbn = {1916-9736 (Print){\textbackslash}r1916-9736 (Linking)},
  pmid = {26383221},
  keywords = {antibiotic resistance,esbl,mrsa,nosocomial infection,open heart surgery},
  file = {/Users/stevenaustin/Zotero/storage/KN99UC7I/GJHS-8-288.pdf}
}

@article{decolliActiveSiteHistidines2019,
  title = {Active {{Site Histidines Link Conformational Dynamics}} with {{Catalysis}} on {{Anti-Infective Target}} 1-{{Deoxy-}} {\textsc{d}} -Xylulose 5-{{Phosphate Synthase}}},
  shorttitle = {Active {{Site Histidines Link Conformational Dynamics}} with {{Catalysis}} on {{Anti-Infective Target}} 1-{{Deoxy-}}},
  author = {DeColli, Alicia A. and Zhang, Xu and Heflin, Kathryn L. and Jordan, Frank and Freel Meyers, Caren L.},
  year = {2019},
  journal = {Biochemistry},
  volume = {58},
  number = {49},
  pages = {4970--4982},
  issn = {0006-2960, 1520-4995},
  doi = {10.1021/acs.biochem.9b00878},
  urldate = {2024-07-02},
  abstract = {The product of 1-deoxy-D-xyluose 5-phosphate (DXP) synthase, DXP, feeds into the bacterial biosynthesis of isoprenoids, thiamin diphosphate (ThDP), and pyridoxal phosphate. DXP is essential for human pathogens but not utilized by humans; thus, DXP synthase is an attractive anti-infective target. The unique ThDP-dependent mechanism and structure of DXP synthase offer ideal opportunities for selective targeting. Upon reaction with pyruvate, DXP synthase uniquely stabilizes the predecarboxylation intermediate, C2{$\alpha$}-lactylThDP (LThDP), in a closed conformation. Subsequent binding of Dglyceraldehyde 3-phosphate induces an open conformation that is proposed to destabilize LThDP, triggering decarboxylation. Evidence for the closed and open conformations has been revealed by hydrogen-deuterium exchange mass spectrometry and X-ray crystallography, which indicate that H49 and H299 are involved in conformational dynamics and movement of the fork and spoon motifs away from the active site is important for the closed-to-open transition. Interestingly, H49 and H299 are critical for DXP formation and interact with the predecarboxylation intermediate in the closed conformation. H299 is removed from the active site in the open conformation of the postdecarboxylation state. In this study, we show that substitution at H49 and H299 negatively impacts LThDP formation by shifting the conformational equilibrium of DXP synthase toward an open conformation. We also present a method for monitoring the dynamics of the spoon motif that uncovered a previously undetected role for H49 in coordinating the closed conformation. Overall, our results suggest that H49 and H299 are critical for the closed, predecarboxylation state providing the first direct link between catalysis and conformational dynamics.},
  copyright = {https://doi.org/10.15223/policy-029},
  langid = {english},
  file = {/Users/stevenaustin/Zotero/storage/K9C5EJNB/DeColli et al. - 2019 - Active Site Histidines Link Conformational Dynamic.pdf}
}

